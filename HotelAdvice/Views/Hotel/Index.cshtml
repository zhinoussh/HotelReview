@using PagedList
@model IPagedList<HotelAdvice.ViewModels.HotelViewModel>
@{
    ViewBag.Title = "Hotel Management";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}


<div id="MyModal"></div>


<h2>City List</h2>

<div class="alert alert-success" role="alert" id="div_alert" style="display:none;">
    <a href="#" id="close_alert" class="close">&times;</a>
    <label id="alert_success" style="margin-right:5px"></label>
</div>
<div class="form-group">
    <button type="button" class="btn btn-success" id="btn_add_new" data-url="/Hotel/ADD_New_Hotel">
        <span class="glyphicon glyphicon-plus" aria-hidden="true"></span> Add New
    </button>
</div>

<div id="table_container">

    @Html.Partial("_PartialHotelList", Model)
</div>